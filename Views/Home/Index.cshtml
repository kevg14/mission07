@{
}

@model BooksViewModel

<h1>Books</h1>

@foreach (Book p in Model.Books)
{
    <h3>@p.Title</h3>

        <ul>
            <li>Book Author: @p.Author</li>
            <li>Publisher: @p.Publisher</li>
            <li>ISBN: @p.Isbn</li>
            <li>Classification: @p.Classification</li>
            <li>Category: @p.Category</li>
            <li>Page Count: @p.PageCount</li>
            <li>Price: @p.Price</li>
        </ul>
        <br />

}

<div page-non="@Model.pageInfo" page-action=""></div>